extends _layout

block title
    title Title of template


block content
    main#barba-wrapper
        section.barba-container(data-namespace="content-page")
            section.container-wrapper-second-header
                .block-img(data-parallax="scroll" data-image-src="img/bg-header/bg1.png")
                .block-second-header.hidden-xs
                    .wrapper-second-header
                        .wrapper-second-header_lang
                            #block-lang.block-dropdown_lang(tabindex="1") Ru
                                ul.dropdown
                                    li
                                        a(href='#') En
                        .wrapper-second-header_link
                            include includes/_social
                            .block-gos-site
                                #block-gos-site.wrapper-gos-site(tabindex="1")
                                    .wrapper-gos-site_title
                                        img.gos-site-logo(src='img/gos-site.png')
                                        span Государственные сайты
                                    ul.dropdown
                                        li
                                            a(href='#') Президент Республики Беларусь
                                        li
                                            a(href='#') Совет Министров
                                        li
                                            a(href='#') Совет Республики Беларусь
                                        li
                                            a(href='#') Национальное собрание Республики Беларусь
                            .wrapper-second-header_personal-area
                                a.link-personal-area(href='#')
                                    span.link-personal-area_icon
                                        <svg width="11.1" height="14.2" xmlns="http://www.w3.org/2000/svg">
                                        <style type="text/css">.st0{fill:#a5a8b0;}</style>
                                        <g>
                                        <g id="svg_1">
                                        <circle id="svg_2" r="3.5" cy="3.5" cx="5.6" class="st0"/>
                                        <path id="svg_3" d="m7.7,8.1c-0.6,0.3 -1.4,0.5 -2.1,0.5s-1.5,-0.2 -2.2,-0.5c-2,1 -3.4,3.3 -3.4,6.1c2,0 9.1,0 11.1,0c0,-2.8 -1.4,-5.1 -3.4,-6.1z" class="st0"/>
                                        </g>
                                        </g>
                                        </svg>
                                    span.link-personal-area_link Кабинет абитуриента
                .block-wrapper-title
                    .block-wrapper-title_title-bread-crumbs
                        h1.title Поиск
                        .bread-crumbs
                            a(href='#') Главная
                            span Поиск
                .block-wrapper-title_name-ac Академия управления <br> при Президенте Республики Беларусь
            section.container-wrapper-content
                .block-wrapper-content
                    .content-search-wrapper
                        .block-search-input
                            form.js-form
                                input.input(type='text' placeholder='Поиск по сайту академии' data-validation="required")
                                button.btn-search
                                    <svg width="20.6" height="20.6" xmlns="http://www.w3.org/2000/svg">
                                    <style type="text/css">.st0{fill:#000000;}</style>
                                    <g>
                                    <path id="svg_1" d="m20.5,19.1l-5.3,-5.3c1.1,-1.5 1.8,-3.3 1.8,-5.3c0,-4.7 -3.8,-8.5 -8.5,-8.5c-4.7,0 -8.5,3.8 -8.5,8.5c0,4.7 3.8,8.5 8.5,8.5c2,0 3.8,-0.7 5.3,-1.8l5.3,5.3c0.1,0.1 0.3,0.1 0.4,0l1.1,-1.1c0,-0.1 0,-0.2 -0.1,-0.3zm-16.6,-5.9c-1.2,-1.2 -1.9,-2.9 -1.9,-4.7c0,-1.8 0.7,-3.4 1.9,-4.6c1.2,-1.2 2.8,-1.9 4.6,-1.9c1.8,0 3.4,0.7 4.6,1.9c1.2,1.2 1.9,2.8 1.9,4.6c0,1.8 -0.7,3.4 -1.9,4.6c-1.2,1.2 -2.8,1.9 -4.6,1.9c-1.8,0.1 -3.4,-0.7 -4.6,-1.8z" class="st0"/>
                                    </g>
                                    </svg>

                                .map-site-search-filter
                                    .map-site-search-filter_radio.black-txt
                                        input(type='radio' name='search' id ='radio-search-1' checked="checked")
                                        label(for='radio-search-1') Поиск по сайту
                                        input(type='radio' name='search' id ='radio-search-2')
                                        label(for ='radio-search-2') Поиск по пресс-центру
                        .wrapper-search-results
                            .three-title Найдено 52 результата
                            .container-search-results
                                .content-search
                                    .block-search-results
                                        a(href='#').block-search-results_title Проходной балл
                                        p.block-search-results_txt Информация о <a href="#">проходных баллах</a> на дневную форму получения высшего образования за счет средств республиканского бюджета.
                                        .block-search-results_data-bread-crumbs
                                            .block-search-results_data 18 апреля 2017
                                            .block-search-results_bread-crumbs
                                                <a href="#">Главная</a> <a href="#">Поступающим</a> <a href="#">Абитуриенту</a>
                                    .block-search-results
                                        a(href='#').block-search-results_title Вопросы ректору / Поступление
                                        p.block-search-results_txt Здравствуйте. Скажите, пожалуйста, каков план приема в Академию управления в 2015 году для получения высшего образования по специальности «Управление информационными ресурсами»? Какой <a href='#'>проходной</a> балл на данную специальность? Спасибо.
                                        .block-search-results_data-bread-crumbs
                                            .block-search-results_data 18 апреля 2017
                                            .block-search-results_bread-crumbs
                                                <a href="#">Главная</a> <a href="#">Поступающим</a> <a href="#">Абитуриенту</a>
                                    .block-search-results
                                        a(href='#').block-search-results_title Вопросы ректору / Поступление
                                        p.block-search-results_txt Здравствуйте. В связи с появлением информации о <a href='#'>проходном балле</a> от министерства образования о поступлении на эконом.специальности без вступительных экзаменов медалистам можно ли поступить к Вам на эконом.факультеты без ЦТ?
                                        .block-search-results_data-bread-crumbs
                                            .block-search-results_data 18 апреля 2017
                                            .block-search-results_bread-crumbs
                                                <a href="#">Главная</a> <a href="#">Поступающим</a> <a href="#">Абитуриенту</a>
                                .block-search-results-nav
                                    ul
                                        li.btn-prev
                                            a(href='#')
                                        li.number
                                            a(href='#') 1
                                        li.interval
                                            a(href='#') ...
                                        li.number
                                            a(href='#') 4
                                        li.number.active
                                            a(href='#') 5
                                        li.number
                                            a(href='#') 6
                                        li.interval
                                            a(href='#') ...
                                        li.number
                                            a(href='#') 12
                                        li.btn-next
                                            a(href='#')
            section.container-wrapper-content.grey
                a(href='#').box-big-link
                    .block-wrapper-content
                        .wrapper-look-further.revealator-zoomin.revealator-once
                            .wrapper-look-further_txt Смотрите далее:
                            .wrapper-look-further_link задать вопрос ректору
            section.container-wrapper-content
                .block-wrapper-content
                    .wrapper-interview-s-n.revealator-zoomin.revealator-once
                        form.wrapper-interview
                            .wrapper-interview_title Опросы
                            .wrapper-interview_q Чем привлекло Вас обучение в Академии <br> управления?
                            .wrapper-interview_block-check
                                .block-check
                                    .block-check-inline
                                        input(type='radio' name='search' id ='radio-interview-s-n-1')
                                        label(for='radio-interview-s-n-1') Высокий статус вуза
                                        .block-bg-input
                                .block-check
                                    .block-check-inline
                                        input(type='radio' name='search' id ='radio-interview-s-n-2')
                                        label(for ='radio-interview-s-n-2') Качество образования
                                        .block-bg-input
                                .block-check
                                    .block-check-inline
                                        input(type='radio' name='search' id ='radio-interview-s-n-3')
                                        label(for ='radio-interview-s-n-3') Качество образования
                                        .block-bg-input
                                .block-check
                                    .block-check-inline
                                        input(type='radio' name='search' id ='radio-interview-s-n-4')
                                        label(for ='radio-interview-s-n-4') Отзывы выпускников
                                        .block-bg-input
                            .wrapper-interview_btn
                                button Ответить
                            .wrapper-interview_number-people В опросе приняло участие  1 124 человек(а)
                        .wrapper-s-n
                            .wrapper-interview_title Академия управления <br> в социальных сетях
                            .wrapper-s-n_tab
                                ul.tabs-s
                                    li.tab-link(data-tabs="tabs-1") facebook
                                    li.tab-link.current-s(data-tabs="tabs-2") вконтакте
                                    li.tab-link(data-tabs="tabs-3") instagram
                                #tabs-1.tab-content-s
                                    .wrapper-tab-content
                                        img(src='img/img-soc-plug.png' alt='')
                                #tabs-2.tab-content-s.current-s
                                    .wrapper-tab-content
                                        img(src='img/img-soc-plug.png' alt='')
                                #tabs-3.tab-content-s
                                    .wrapper-tab-content
                                        img(src='img/img-soc-plug.png' alt='')
                    .wrapper-banners.revealator-zoomin.revealator-once
                        ul
                            li
                                a(href='#')
                                    img(src='img/baner/baner-1.png' alt='')
                            li
                                a(href='#')
                                    img(src='img/baner/baner-2.png' alt='')
                            li
                                a(href='#')
                                    img(src='img/baner/baner-3.png' alt='')
                            li
                                a(href='#')
                                    img(src='img/baner/baner-4.png' alt='')

