            .container-priomnaya-comp-table .table_inner {
                border-collapse: collapse;
                empty-cells: show;
                table-layout: fixed;
                border-bottom: 2px solid #b4c2d8;
            }

            .container-priomnaya-comp-table .table_inner .table-head {
                display: table-row-group;
                vertical-align: top;
